    "format": "prettier --write src/**/**.js src/**/**.vue",
    "prod": "serve -s dist"
  },
  "author": "SkyBeast",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.8",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.8",
    "@vue/cli-service": "^3.0.0-rc.8",
    "prettier": "^1.14.0",
    "serve": "^9.4.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
